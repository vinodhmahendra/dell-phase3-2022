package simplilearn.entity;

public class Application {
	
	private Integer id;
	private String name;
	private String descripton;
	private String owner;
	
	public Application() {
		
	}
	
	public Application(String name,String owner,String description) {
		this.name = name;
		this.owner = owner;
		this.descripton = description;
	}

}
